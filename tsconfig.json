{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"],
      "@lib/*": ["src/lib/*"],
      "@icons/*": ["src/assets/icons/*"],
      "@store/*": ["src/store/*"],
    },
  },
  "include": ["src/**/*.js", "src/**/*.jsx", "src/**/*.ts", "src/**/*.tsx", "src/**/*.astro"],
  "extends": "astro/tsconfigs/strict"
}